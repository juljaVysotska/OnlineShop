<Window x:Class="OnlineShop.NewPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OnlineShop"
        mc:Ignorable="d"
        Title="NewPage" Height="590" Width="970">
    <Grid>
        <Label HorizontalContentAlignment="Center" Margin="0,20,0,0" FontFamily="Segoe Print" FontSize="36">ArtMart</Label>
        <Button x:Name="btnPaint" Width="200" Height="30" Margin="100,100,0,0" HorizontalAlignment="Left" VerticalAlignment="Top">Paint</Button>
        <Button x:Name="btnPaper" Width="200" Height="30" Margin="0,100,0,0" HorizontalAlignment="Center" VerticalAlignment="Top">Paper</Button>
        <Button x:Name="btnTassel" Width="200" Height="30" Margin="0,100,100,0" HorizontalAlignment="Right" VerticalAlignment="Top">Tassel</Button>
    </Grid>
</Window>
